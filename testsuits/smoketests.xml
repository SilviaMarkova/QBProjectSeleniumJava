<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="SmokeSuite">
    <groups>
        <run>
            <include name="Smoke"/>
        </run>
    </groups>
    <test name="SmokeTests">
        <classes>
            <class name="org.selenium.tests.searchtests.CorrectSearchResultsOnExactSearchMatchTest"/>
            <class name="org.selenium.tests.navigationtests.VerifyGithubLinkTest"/>
        </classes>
    </test>
</suite>